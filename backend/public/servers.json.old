{
    <Discord Guild ID>: {
        <random server UUID>: {
            channel: <discord guild channel ID>,
            server_info: {
                server_ip: <server ip>,
                server_connection_port: <connection port>,
                server_query_port: <query port>,
                server_game: <game>
                server_protocol: <server protocol>
            },
            embed_settings: {
                embed_color: <embed color>,
                embed_title: <embed title>,
                embed_description: <embed description>,
                disable_timestamp: <true | false>,
                thumbnail_settings: {
                    disable_thumbnail: <true | false>,
                    thumbnail_url: <thumbnail URL>
                },
                footer_settings: {
                    disable_footer: <true | false>,
                    footer_text: <footer text>,
                    footer_url: <footer url>
                }
            },
            embed_fields: {
                status: {
                    disable_status: <true | false>,
                    status_index: <status index>,
                    status_online_emoji: <emoji>,
                    status_offline_emoji: <emoji>,
                    status_text: <status text>
                },
                connection: {
                    disable_connection: <true | false>,
                    connection_index: <connection index>,
                    connection_text: <connection text>
                },
                location: {
                    disable_location: <true | false>,
                    location_index: <location index>,
                    location_emoji: <emoji>,
                    location_text: <location text>
                },
                game: {
                    disable_game: <true | false>,
                    game_index: <game index>,
                    game_text: <game>
                },
                map: {
                    disable_map: <true | false>,
                    map_index: <map index>,
                    map_text: <map text>
                },
                players: {
                    disable_players: <true | false>,
                    players_index: <players index>,
                    players_text: <players text>
                },
                password_protected: {
                    disable_password_protected: <true | false>,
                    password_protected_index: <password_protected index>,
                    password_protected_true_emoji: <emoji>,
                    password_protected_text: <password_protected text>
                },
                password: {
                    disable_password: <true | false>,
                    password_index: <password index>,
                    password_text: <password text>
                },
                custom: {
                    disable_custom: <true | false>,
                    custom_index: <custom index>,
                    custom_title: <custom title>,
                    custom_content: <custom content>
                },
                ... (user can make unlimited custom fields)
                disable_player_names: <true | false>
            },
            graph_settings: {
                disable_graph: <true | false>,
                title_settings: {
                    title_text: <title text>,
                    title_color: <title color>
                },
                x_label_settings: {
                    x_label_title: <X Label Title>,
                    x_label_color: <X Label Color>
                },
                y_label_settings: {
                    y_label_text: <Y Label Title>,
                    y_label_color: <Y Color Color>
                },
                tick_settings: {
                    x_tick_color: <x tick color>,
                    y_tick_color: <y tick color>
                },
                graph_line_settings: {
                    player_settings: {
                        disable_fill: <true | false>,
                        line_color: <player line color>,
                        fill_color: <player fill color>,
                    },
                    trend_settings: {
                        disable_trend: <true | false>,
                        trend_color: <trend color>
                    },
                    grid_settings: {
                        disable_grid: <true | false>,
                        grid_color: <grid color>,
                        grid_opacity: <grid opacity>
                    }
                },
                legend_settings: {
                    disable_legend: <true | false>,
                    legend_background_color: <legend background color>,
                    legend_background_opacity: <legend background opacity>,
                    legend_border_color: <legend border color>,
                    legend_border_opacity: <legend border opacity>
                },
                timeframe: <timeframe>,
                graph_type: <graph type>
            },
            notification_settings: {
                disable_notifications: <true | false>,
                disable_timestamp: <true | false>,
                notification_channel_id: <channel id>,
                embed_settings: {
                    embed_title: <embed title>,
                    embed_description: <embed description>,
                    embed_color: {
                        embed_offline_color: <embed offline color>,
                        embed_online_color: <embed online color>
                    },
                    thumbnail_settings: {
                        disable_thumbnail: <true | false>,
                        thumbnail_url: <thumbnail url>,
                    },
                    footer_settings: {
                        disable_footer: <true | false>,
                        footer_text: <footer text>,
                        footer_url: <footer url>,
                    },
                    field_settings: {
                        status: {
                            disable_status: <true | false>,
                            status_index: <status index>,
                            status_online_emoji: <status online emoji>,
                            status_offline_emoji: <status offline emoji>,
                            status_text: <online | offline>
                        }
                    },
                    ping_settings: {
                        disable_ping: <true | false>,
                        ping_user: <ping user>,
                    }
                }
            }
        }
    }
}